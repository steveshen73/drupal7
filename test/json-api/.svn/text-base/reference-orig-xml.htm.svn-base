<P CLASS="DisplayCodeAcross">
<a name="pgfId-1003989"></a>      System.out.println(send(url, request));</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1003990"></a>&nbsp;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1003991"></a>      } catch (Throwable t) {</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1003992"></a>         t.printStackTrace();</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1003993"></a>      }</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1003994"></a>   }</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1003995"></a>&nbsp;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1003996"></a>   public static String send(String url1, String content) throws Exception {</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1003997"></a>      try {</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1003998"></a>         URL url = new URL(url1);</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1003999"></a>         URLConnection urlConn = url.openConnection();</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004000"></a>&nbsp;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004001"></a>         urlConn.setDoInput (true);</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004002"></a>         urlConn.setDoOutput (true);</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004003"></a>         urlConn.setRequestProperty(&quot;Content-Type&quot;, &quot;application/x-www-form-urlencoded&quot;);</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004004"></a>         urlConn.setRequestProperty(&quot;Content-Length&quot;, content.length()+&quot;&quot;);</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004005"></a>&nbsp;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004006"></a>         ((HttpURLConnection) urlConn).setRequestMethod(&quot;POST&quot;);</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004007"></a>&nbsp;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004008"></a>         OutputStream os1 = urlConn.getOutputStream ();</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004009"></a>         DataOutputStream  os = new DataOutputStream (os1);</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004010"></a>&nbsp;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004011"></a>         os.writeBytes(content);</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004012"></a>&nbsp;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004013"></a>         os.flush();</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004014"></a>         os.close();</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004015"></a>&nbsp;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004016"></a>         StringBuilder responseBuffer = receive(urlConn.getInputStream());</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004017"></a>         String response = getXmlContent(responseBuffer);</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004018"></a>         return response;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004019"></a>      } catch (IOException e) {</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004020"></a>         e.printStackTrace();</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004021"></a>         throw e;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004022"></a>      }</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004023"></a>   }</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004024"></a>&nbsp;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004025"></a>   private static StringBuilder receive(InputStream is) throws IOException {</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004026"></a>      BufferedReader fromServer = null;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004027"></a>      StringBuilder response = new StringBuilder();</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004028"></a>&nbsp;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004029"></a>      try {</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004030"></a>         fromServer = new BufferedReader(new InputStreamReader(is));</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004031"></a>         String inputLine = &quot;&quot;;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004032"></a>         while ((inputLine = fromServer.readLine()) != null) {</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004033"></a>            response.append(inputLine);</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004034"></a>         }</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004035"></a>         return response;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004036"></a>&nbsp;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004037"></a>      } finally {</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004038"></a>         if (fromServer != null) {</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004039"></a>            fromServer.close();</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004040"></a>         }</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004041"></a>      }</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004042"></a>   }</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004043"></a>&nbsp;</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004044"></a>   private static String getXmlContent(StringBuilder response) {</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004045"></a>      int startpos = response.indexOf(PREFIX_XML);</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004046"></a>      return response.substring(startpos);</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004047"></a>   }</P>
<P CLASS="DisplayCodeAcross">
<a name="pgfId-1004049"></a>}</P>
