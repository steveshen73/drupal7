<?php
if (file_exists('test.xml'))
  {
  $xml = simplexml_load_file('test.xml'); //The simplexml_load_file() function loads an XML document into an object.
  echo "<pre>";
  var_dump($xml);
  echo "</pre>";

  echo "</br>";
  echo $xml->from;
  
  echo "</br>";
  echo $xml->body;

  }

else
  {
  exit('Error.');
  }
?> 
